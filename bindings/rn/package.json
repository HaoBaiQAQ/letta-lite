{
  "name": "letta-lite",
  "version": "0.1.0",
  "description": "Portable on-device Letta agents for React Native",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/src/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "letta-lite.podspec",
    "!lib/typescript/example",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "typescript": "tsc --noEmit",
    "prepare": "bob build",
    "release": "release-it"
  },
  "keywords": [
    "react-native",
    "ios",
    "android",
    "letta",
    "ai",
    "agent",
    "llm"
  ],
  "repository": "https://github.com/letta-ai/letta-lite",
  "author": "Letta Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/letta-ai/letta-lite/issues"
  },
  "homepage": "https://github.com/letta-ai/letta-lite#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@react-native-community/eslint-config": "^3.0.0",
    "@types/jest": "^29.5.0",
    "@types/react": "^18.2.0",
    "@types/react-native": "^0.72.0",
    "eslint": "^8.50.0",
    "jest": "^29.5.0",
    "react": "18.2.0",
    "react-native": "0.72.0",
    "react-native-builder-bob": "^0.23.0",
    "release-it": "^16.0.0",
    "typescript": "^5.2.0"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
}